{
  "name": "sc-market-image-lambda",
  "version": "1.0.0",
  "description": "AWS Lambda for image moderation and upload to Backblaze",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "package": "./build.sh",
    "deploy": "yarn package && aws lambda update-function-code --function-name sc-market-image-lambda --zip-file fileb://dist/lambda.zip",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "lint": "yarn format:check",
    "test:large-image": "node test-large-image.js"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "sharp": "^0.33.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.5.3"
  },
  "private": true
}
